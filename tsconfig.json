{
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "paths": {
            "src/*": ["./src/*"]
        },
        "resolveJsonModule": true
    }
}